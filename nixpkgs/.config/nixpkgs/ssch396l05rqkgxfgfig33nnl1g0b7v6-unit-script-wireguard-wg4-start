#! /nix/store/715fljdrspphbdk255r729zxp4w7lcgk-bash-4.4-p23/bin/bash -e
modprobe wireguard || true



ip link add dev wg4 type wireguard


ip address add 2404:7a80:32e1:6600:20c::1/68 dev wg4

wg set wg4 private-key /etc/nixos/wireguard-private-key  listen-port 52337

ip link set up dev wg4

